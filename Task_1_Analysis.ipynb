# this is different code 
import xarray as xr
import matplotlib.pyplot as plt

# -------------------------------------------------
# STEP 1: Load Xarray dataset
# -------------------------------------------------
ds = xr.open_dataset("D:\Osdag_Project\data\screening_task.nc")

# -------------------------------------------------
# STEP 2: Central girder element list
# -------------------------------------------------
central_girder = [15, 24, 33, 42, 51, 60, 69, 78, 83]

# -------------------------------------------------
# STEP 3: Prepare lists for plotting
# -------------------------------------------------
x_positions = []
Mz_values = []
Vy_values = []

current_x = 0

# -------------------------------------------------
# STEP 4: Extract data WITHOUT duplication
# -------------------------------------------------
for i, elem in enumerate(central_girder):
    Mz_i = ds["forces"].sel(Component="Mz_i", Element=elem).values
    Mz_j = ds["forces"].sel(Component="Mz_j", Element=elem).values
    Vy_i = ds["forces"].sel(Component="Vy_i", Element=elem).values
    Vy_j = ds["forces"].sel(Component="Vy_j", Element=elem).values

    if i == 0:
        # First element → take both start & end
        x_positions.append(current_x)
        Mz_values.append(Mz_i)
        Vy_values.append(Vy_i)

    # Always add end node
    current_x += 1
    x_positions.append(current_x)
    Mz_values.append(Mz_j)
    Vy_values.append(Vy_j)

# -------------------------------------------------
# STEP 5: Plot Shear Force Diagram (SFD)
# -------------------------------------------------
plt.figure(figsize=(8, 4))
plt.plot(x_positions, Vy_values, marker='o', color='blue')
plt.axhline(0, color='black', linewidth=0.8)
plt.title("Shear Force Diagram (Central Girder)")
plt.xlabel("Position along Girder")
plt.ylabel("Shear Force (Vy)")
plt.grid(True)
plt.tight_layout()
plt.show()

# -------------------------------------------------
# STEP 6: Plot Bending Moment Diagram (BMD)
# -------------------------------------------------
plt.figure(figsize=(8, 4))
plt.plot(x_positions, Mz_values, marker='o', color='red')
plt.axhline(0, color='black', linewidth=0.8)
plt.title("Bending Moment Diagram (Central Girder)")
plt.xlabel("Position along Girder")
plt.ylabel("Bending Moment (Mz)")
plt.grid(True)
plt.tight_layout()
plt.show()

# this is anothore code 
import xarray as xr
import matplotlib.pyplot as plt
import numpy as np

# -------------------------------------------------
# STEP 1: Load Xarray dataset
# -------------------------------------------------
ds = xr.open_dataset("D:\Osdag_Project\data\screening_task.nc")

# -------------------------------------------------
# STEP 2: Central girder element list
# -------------------------------------------------
central_girder = [15, 24, 33, 42, 51, 60, 69, 78, 83]

# -------------------------------------------------
# STEP 3: Prepare lists for plotting
# -------------------------------------------------
x_positions = []
Mz_values = []
Vy_values = []

current_x = 0

# -------------------------------------------------
# STEP 4: Extract data WITHOUT duplication
# -------------------------------------------------
for i, elem in enumerate(central_girder):
    Mz_i = ds["forces"].sel(Component="Mz_i", Element=elem).values
    Mz_j = ds["forces"].sel(Component="Mz_j", Element=elem).values
    Vy_i = ds["forces"].sel(Component="Vy_i", Element=elem).values
    Vy_j = ds["forces"].sel(Component="Vy_j", Element=elem).values

    if i == 0:
        # First element → take both start & end
        x_positions.append(current_x)
        Mz_values.append(Mz_i)
        Vy_values.append(Vy_i)

    # Always add end node
    current_x += 1
    x_positions.append(current_x)
    Mz_values.append(Mz_j)
    Vy_values.append(Vy_j)

# Convert list of arrays to flat numpy arrays for plotting
x_plot = np.array(x_positions)
bmd_values = np.array(Mz_values).flatten()
sfd_values = np.array(Vy_values).flatten()

# -------------------------------------------------
# STEP 5 & 6: Plot Shear Force Diagram (SFD) and Bending Moment Diagram (BMD)
# -------------------------------------------------
fig, (ax1, ax2) = plt.subplots(2, 1, figsize=(10, 8), sharex=True)

# Bending Moment Diagram (BMD)
ax1.plot(x_plot, bmd_values, color='red', linewidth=2, label='Mz (kNm)')
ax1.fill_between(x_plot, 0, bmd_values, color='red', alpha=0.3)
ax1.set_title('Task 1: Bending Moment Diagram (Central Girder)')
ax1.set_ylabel('Moment (kNm)')
ax1.grid(True, linestyle='--')
ax1.axhline(0, color='black', linewidth=0.8)
ax1.legend()

# Shear Force Diagram (SFD)
ax2.plot(x_plot, sfd_values, color='blue', linewidth=2, label='Vy (kN)')
ax2.fill_between(x_plot, 0, sfd_values, color='blue', alpha=0.3)
ax2.set_title('Task 1: Shear Force Diagram (Central Girder)')
ax2.set_xlabel('Distance along Bridge (X-coordinate)')
ax2.set_ylabel('Shear Force (kN)')
ax2.grid(True, linestyle='--')
ax2.axhline(0, color='black', linewidth=0.8)
ax2.legend()

plt.tight_layout()
plt.show()
